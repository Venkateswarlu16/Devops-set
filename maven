#Linux
sudo apt update -y
sudo apt install openjdk-8-jdk maven tree git -y
java -version
mvn -v
sudo update-alternatives --config java

git clone https://github.com/devopsbyraham/jenkins-java-project.git
cd jenkins-java-project
ls | grep pom.xml


